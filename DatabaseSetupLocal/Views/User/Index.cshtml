@using DatabaseSetupLocal.Data
@model List<User>

<div>

    <table>
        <tr>
            <th>
                @Html.DisplayNameFor(Model => Model.First().UserName)
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.UserName)
                </td>

                <td>
                    @Html.ActionLink("Races", "Races", new {userId = item.Id}) |
                </td>
            </tr>
        }
    </table>

</div>